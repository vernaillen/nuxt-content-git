{
  "name": "nuxt-content-git",
  "version": "4.0.0",
  "description": "Additional module for @nuxt/content that replaces or adds createdAt and updatedAt dates based on the git history.",
  "keywords": [
    "frontend",
    "javascript",
    "node",
    "nodejs",
    "npm",
    "npm-package",
    "nuxt",
    "nuxt-content",
    "nuxt-module",
    "nuxtjs",
    "nuxtjs-module",
    "webdevelopment"
  ],
  "repository": "dword-design/nuxt-content-git",
  "funding": "https://github.com/sponsors/dword-design",
  "license": "MIT",
  "author": "Sebastian Landwehr <info@sebastianlandwehr.com>",
  "type": "module",
  "exports": {
    ".": {
      "types": "./dist/types.d.ts",
      "import": "./dist/module.mjs",
      "require": "./dist/module.cjs"
    }
  },
  "main": "./dist/module.cjs",
  "types": "./dist/types.d.ts",
  "files": [
    "dist"
  ],
  "scripts": {
    "dev": "nuxi dev examples/documentDriven",
    "build": "nuxi build examples/documentDriven",
    "prepare": "nuxt-module-build --stub && nuxi prepare examples/documentDriven",
    "lint": "eslint .",
    "prepack": "nuxt-module-build",
    "test": "vitest test"
  },
  "dependencies": {
    "@nuxt/content": "^2.4.3",
    "@nuxt/kit": "^3.2.3",
    "simple-git": "^3.17.0"
  },
  "devDependencies": {
    "@nuxt/module-builder": "^0.2.1",
    "@nuxt/schema": "^3.2.3",
    "@nuxt/ui": "^0.4.1",
    "@nuxtjs/eslint-config-typescript": "latest",
    "nuxt": "^3.2.3",
    "typescript": "^4.9.5",
    "vitest": "^0.29.2"
  },
  "engines": {
    "node": ">=14"
  },
  "publishConfig": {
    "access": "public"
  }
}
